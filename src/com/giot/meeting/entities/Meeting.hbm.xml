<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.giot.meeting.entities.Meeting" table="MEETING">
		<id name="meetid" type="java.lang.String">
			<column name="MEETID" />
			<generator class="uuid" />
		</id>
		<property name="title" type="java.lang.String">
			<column name="TITLE" />
		</property>
		<property name="content" type="text">
			<column name="CONTENT" />
		</property>
		<property name="duration" type="java.lang.String">
			<column name="DURATION" />
		</property>
		<property name="visible" type="java.lang.String">
			<column name="VISIBLE" />
		</property>
		<property name="organiser" type="java.lang.String">
			<column name="ORGANISER" />
		</property>
		<property name="remark" type="java.lang.String">
			<column name="REMARK"></column>
		</property>
		<property name="location" type="java.lang.String">
			<column name="LOCATION"></column>
		</property>
	</class>
</hibernate-mapping>
